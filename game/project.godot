animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
layout/style_list=["res://dialog/dialog_style.tres"]
layout/default_style="res://dialog/dialog_style.tres"

[dialogue_manager]

general/states=["GameState", "DialogueState"]
general/uses_dotnet=true
general/balloon_path="res://src/features/dialogue/Balloon.tscn"

[display]

window/size/viewport_width=320
window/size/viewport_height=180
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"

[dotnet]

project/assembly_name="The-adventure-of-hink"

[editor]

movie_writer/movie_file="/Users/mattias800/Downloads/hink.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

